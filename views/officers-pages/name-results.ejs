<%- include("../partials/header.ejs") %>

<h1 class="card-title"> <%= result.first_name %> <%= result.last_name %></h1>
<div class="users-list">
    <ul class="list-group">
        <li class="list-group-item">First Name: <%= result.first_name %> </li>
        <li class="list-group-item">Last Name: <%= result.last_name %></li>
        <li class="list-group-item">Gender: <%= result.gender %></li>
        <li class="list-group-item">Birthday: <%= result.birth %> </li>
        <li class="list-group-item">Ethnicity: <%= result.ethnicity %> </li>
        <li class="list-group-item">Hair Color: <%= result.hair %> </li>
        <li class="list-group-item">Eye Color: <%= result.eyes %> </li>
        <li class="list-group-item">Address: <%= result.address %> </li>
        <li class="list-group-item">Height: <%= result.height %> </li>
    </ul>
</div>