<%- include("../partials/header.ejs") %> <%-
include("../partials/admin-header.ejs") %>

<form class="edit-veh-form" action="" method="post" enctype="application/x-www-form-urlencoded">
  <div class="form-group">
    <label for="exampleInputEmail1">Enter Vehicle Name</label>
    <input name="cname" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"
      placeholder="Car Name" value="<%= vehicle.cname %>" />
  </div>
  <div class="form-group">
    <label for="exampleInputEmail1">Enter Vehicle Make Year</label>
    <input name="myear" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"
      placeholder="Make Year" value="<%= vehicle.myear %>" />
  </div>
  <button type="submit" class="btn btn-success float-right">Update</button>
</form>