<body>
  <nav class="navbar navbar-expand-lg navbar-light bg-secondary">
    <a class="navbar-brand text-light" href="/">Equinox CAD</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
      aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon text-light"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item">
          <a class="nav-link text-light" href="/myofficers">Police Dept.</a>
        </li>
        <li class="nav-item">
          <a class="nav-link text-light" href="/myofficers">EMS/FD</a>
        </li>
        <li class="nav-item">
          <a class="nav-link text-light" href="/citizen">Citizens</a>
        </li>
        <% if (isAdmin === true) {%>
        <li class="nav-item">
          <a class="nav-link text-light" href="/admin">Admin </a>
        </li>
        <%} else {%>
        <%} %>
        <!-- <li class="nav-item">
          <a class="btn btn-primary nav-link" href="/officers/tablet">Tablet</a>
        </li> -->
      </ul>
    </div>
  </nav>
  <script>
    function myFunction() {
      var input, filter, ul, li, a, i, txtValue;
      input = document.getElementById("myInput");
      filter = input.value.toUpperCase();
      ul = document.getElementById("myUL");
      li = ul.getElementsByTagName("li");
      for (i = 0; i < li.length; i++) {
        a = li[i];
        txtValue = a.textContent || a.innerText;
        if (txtValue.toUpperCase().indexOf(filter) > -1) {
          li[i].style.display = "";
        } else {
          li[i].style.display = "none";
        }
      }
    }
  </script>

</body>

</html>