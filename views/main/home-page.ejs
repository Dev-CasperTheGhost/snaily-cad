<script>
    function loadMore() {
        let moreBtn = document.getElementById("loadMore")
    let hidden = document.getElementById("hiddenMore")
    let dots = document.getElementById("dots")
    if (dots.style.display === "none") {
        dots.style.display = "block"
        hidden.style.display = "none";
            moreBtn.textContent = "Load More Screenshots"

        } else {
            dots.style.display = "none";
            hidden.style.display = "block";
            moreBtn.textContent = "Load Less Screenshots"
        }
    }
   
</script>