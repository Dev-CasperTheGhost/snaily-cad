<%- include("../partials/header.ejs") %>

<h1 class="card-title"> <%= citizen.first_name %> <%= citizen.last_name %></h1>
<div class="users-list">
    <ul class="list-group">
        <li class="list-group-item">First Name: <%= citizen.first_name %> </li>
        <li class="list-group-item">Last Name: <%= citizen.last_name %></li>
        <li class="list-group-item">Gender: <%= citizen.gender %></li>
        <li class="list-group-item">Birthday: <%= citizen.birth %> </li>
        <li class="list-group-item">Ethnicity: <%= citizen.ethnicity %> </li>
        <li class="list-group-item">Hair Color: <%= citizen.hair %> </li>
        <li class="list-group-item">Eye Color: <%= citizen.eyes %> </li>
        <li class="list-group-item">Address: <%= citizen.address %> </li>
        <li class="list-group-item">Height: <%= citizen.height %> </li>
    </ul>
</div>