<%- include("../partials/header.ejs") %> <%-
include("../partials/admin-header.ejs") %>
<h1 class="users-title card-title">
  Vehicles management <a href="/admin/values/cars/add">+</a>
</h1>

<% if (vehicles.length > 0) {%>
<ul class="list-group">
  <% vehicles.forEach(value => { %>
  <li class="list-group-item">

    <%= value.id %> | <%= value.myear %> | <%= value.cname %>
    <a href="/admin/values/cars/delete/<%= value.id %>" class="btn btn-danger float-right">Delete</a>
    <a href="/admin/values/cars/edit/<%= value.id %>" class="btn btn-success float-right  margin-right-10">Edit</a>
  </li>

  <% }) %>
</ul>
<% } else { %>
<p class="text-center">
  No businesses found. Go <a href="/add">here</a> to add business.
</p>
<% } %>