<%- include("../partials/header.ejs") %> <%-
include("../partials/admin-header.ejs") %>

<form
  class="edit-veh-form text-light"
  action=""
  method="post"
  enctype="application/x-www-form-urlencoded"
>
  <div class="form-group">
    <label for="exampleInputEmail1">Enter Department</label>
    <input
      name="dept"
      type="text"
      class="form-control text-light border-dark bg-dark"
      placeholder="Department"
      value="<%= depts.name %>"
    />
  </div>
  <button type="submit" class="btn btn-success float-right">Update Department</button>
  <a
    class="btn btn-danger cancel-btn float-right"
    href="/cad/<%= cadId %>/admin/values/depts"
    >Cancel</a
  >
</form>
