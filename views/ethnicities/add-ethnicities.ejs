<%- include("../partials/header.ejs") %>
<div class="container-fluid">
  <div class="row">
    <%- include("../partials/admin-header.ejs") %>

    <main role="main" class="col-md-9 ml-sm-auto col-lg-10 text-light mt-2">
      <form action="" method="post" enctype="application/x-www-form-urlencoded">
        <div class="form-group">
          <h1 class="card-title">Add Ethnicity</h1>
          <label for="first-name" class="text-light">Ethnicity</label>
          <input
            type="text"
            class="container form-control text-light bg-dark border-dark"
            name="ethnicity"
            id="first-name"
            placeholder="ethnicity"
            required
          />
        </div>
        
        <button type="submit" class="btn btn-primary float-right">
          Add Ethnicity
        </button>
        <a
          class="btn btn-danger cancel-btn float-right"
          href="/cad/<%= cadId %>/admin/values/ethnicities"
          >Cancel</a
        >
      </form>
    </main>
  </div>
</div>
